package com.accenture.junit;

public class HistoryItem {
	private int id;
	private int amount;
	private String operation;
	private int total;
	public HistoryItem(int id, int amount, String operation, int total) {
		super();
		this.id = id;
		this.amount = amount;
		this.operation = operation;
		this.total = total;
	}
	
	
	
}
